@using Data.Model;

<div class="col-md-3 align-content-center mt-2 mb-2">
    <label id="HowrseActionLabel" class="font-weight-bold">@HowrseActionCheckboxModel.TaskType.ToString()</label>
    <img src="@HowrseActionCheckboxModel.ImageUrl" class="actionImage" />
    <input class="form-check-input align-bottom" type="checkbox" checked="@HowrseActionCheckboxModel.Checked" />
</div>




@code {
    [Parameter] public HowrseActionCheckboxModel HowrseActionCheckboxModel { get; set; }
}
